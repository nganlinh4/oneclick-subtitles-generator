{
  "name": "subtitles-generator",
  "version": "1.0.0",
  "description": "A subtitle generator application using Gemini AI",
  "homepage": "./",
  "workspaces": [
    "promptdj-midi",
    "video-renderer"
  ],
  "scripts": {
    "start": "npm run setup:react-env && npm run generate:version && node scripts/start-frontend.js",
    "start-react": "cross-env PORT=3030 react-scripts start",
    "build": "npm run generate:version && react-scripts build",
    "build:vercel": "CI=false react-scripts build",
    "generate:version": "node scripts/generate-version.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "node server.js",
    "server:start": "node server.js",
    "python:start": "uv run --python .venv server/narrationApp.py",
    "python:test": "uv run --python .venv server/test-narration-service.py",
    "video-renderer:build": "cd video-renderer && npm run server:build",
    "video-renderer:start": "cd video-renderer && npm run server:start",
    "dev": "npm run setup:react-env && npm run generate:version && node dev-server.js",
    "dev:test": "concurrently --names \"FRONTEND,SERVER,PYTHON\" --prefix-colors \"cyan,green,yellow\" --prefix \"[{name}]\" \"npm run start\" \"npm run server:start\" \"npm run python:test\"",
    "check:uv": "node check-uv.js",
    "test:python": "uv run --python .venv test-python.py",
    "create:venv": "uv venv .venv",
    "install:f5tts": "uv pip install --python .venv -e F5-TTS",
    "install:deps": "uv pip install --python .venv flask flask-cors soundfile numpy torch torchaudio vocos setuptools",
    "install:yt-dlp": "node install-yt-dlp.js",
    "install:all": "node utils/npm-quiet.js install && node setup-narration.js && node install-yt-dlp.js",
    "postinstall": "node install-yt-dlp.js && npm run workspace:build",
    "setup:f5tts": "node setup-f5tts.js",
    "python:start:cuda": "uv run --python .venv -- python server/narrationApp.py",
    "dev:cuda": "node scripts/start-all.js",
    "dev:cuda-legacy": "cross-env START_PYTHON_SERVER=true DEV_SERVER_MANAGED=true concurrently --names \"FRONTEND,SERVER\" --prefix-colors \"cyan,green\" --prefix \"[{name}]\" \"npm run start --silent\" \"npm run server:start\"",
    "install:all:uv": "npm install --silent && node setup-narration-uv.js && node install-yt-dlp.js",
    "python:start:uv": "uv run --python .venv -- python server/narrationApp.py",
    "chatterbox:start": "uv run --python .venv python chatterbox-fastapi/start_api.py --reload",
    "test:services": "node test-services.js",
    "test:setup-fixes": "node test-setup-fixes.js",
    "test:port-manager": "node scripts/test-port-manager.js",
    "setup:cors": "node scripts/setup-cors-env.js",
    "setup:react-env": "node scripts/generate-react-env.js",
    "test:cors": "node scripts/test-cors-configuration.js",
    "setup:narration:uv": "node setup-narration.js",
    "setup:narration:quiet": "cross-env QUIET=true node setup-narration.js",
    "setup:narration:verbose": "cross-env VERBOSE=true node setup-narration.js",
    "install:yt-dlp:quiet": "cross-env QUIET=true node install-yt-dlp.js",
    "install:yt-dlp:verbose": "cross-env VERBOSE=true node install-yt-dlp.js",
    "install:all:quiet": "cross-env QUIET=true npm install --silent && cross-env QUIET=true node setup-narration.js && cross-env QUIET=true node install-yt-dlp.js",
    "install:all:verbose": "cross-env VERBOSE=true npm install && cross-env VERBOSE=true node setup-narration.js && cross-env VERBOSE=true node install-yt-dlp.js",
    "start:original": "node scripts/start-frontend.js",
    "workspace:build": "npm run --workspace=video-renderer server:build && npm run --workspace=promptdj-midi build"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@google/genai": "^1.0.0",
    "@material/web": "^2.3.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@react-oauth/google": "^0.12.1",
    "@remotion/bundler": "4.0.341",
    "@remotion/google-fonts": "4.0.341",
    "@remotion/player": "4.0.341",
    "@remotion/renderer": "4.0.341",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-tabs": "^5.0.4",
    "@types/uuid": "^10.0.0",
    "adm-zip": "^0.5.16",
    "assert": "^2.1.0",
    "axios": "^1.8.4",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "crypto-browserify": "^3.12.1",
    "events": "^3.3.0",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.3",
    "genius-lyrics": "^4.4.7",
    "googleapis": "^148.0.0",
    "http-proxy-middleware": "^3.0.5",
    "https-browserify": "^1.0.0",
    "i18next": "^23.8.0",
    "jszip": "^3.10.1",
    "multer": "^1.4.5-lts.2",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "puppeteer": "^24.7.2",
    "puppeteer-core": "^24.7.2",
    "querystring-es3": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.3.0",
    "react-icons": "^5.5.0",
    "react-player": "^2.16.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "react-tabs": "^6.1.0",
    "react-window": "^1.8.11",
    "remotion": "4.0.341",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "styled-components": "^6.1.15",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "uuid": "^11.1.0",
    "web-vitals": "^3.5.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@remotion/cli": "4.0.341",
    "@remotion/eslint-plugin": "4.0.341",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "chalk": "^4.1.2",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "typescript": "^4.9.5"
  },
  "overrides": {
    "remotion": "4.0.341",
    "@remotion/*": "4.0.341"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}